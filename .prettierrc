{
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "none",
  "overrides": [
      {
          "files": "*.json",
          "options": {
              "parser": "json",
              "tabWidth": 2
          }
      },
      {
          "files": "*.yml",
          "options": {
              "parser": "yaml",
              "tabWidth": 2
          }
      }
  ]
}
